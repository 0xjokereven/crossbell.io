{
	"name": "@crossbell/indexer",
	"version": "0.0.1",
	"license": "MIT",
	"repository": "github:Crossbell-Box/crossbell-universe",
	"bugs": "https://github.com/Crossbell-Box/crossbell-universe/issues",
	"homepage": "https://github.com/Crossbell-Box/crossbell-universe#readme",
	"author": "Natural Selection Labs",
	"keywords": [],
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"typings": "dist/esm/index.d.ts",
	"sideEffects": false,
	"files": [
		"dist",
		"package.json"
	],
	"scripts": {
		"build": "npm run clean && run-p build:cjs build:esm",
		"build:cjs": "tsc -p tsconfig.json -m CommonJS --outDir dist/cjs",
		"build:esm": "tsc -p tsconfig.json -m ESNext --outDir dist/esm",
		"clean": "rm -rf ./dist"
	},
	"dependencies": {
		"lru-cache": "7.14.0"
	},
	"peerDependencies": {
		"@tanstack/react-query": "4.x",
		"crossbell.js": "0.11.x",
		"ethers": "5.x",
		"wagmi": "0.7.x"
	}
}
